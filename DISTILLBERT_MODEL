    {
      "cell_type": "code",
      "source": [
        "def compute_metrics(eval_pred):\n",
        "    logits, labels = eval_pred\n",
        "    predictions = np.argmax(logits, axis=-1)\n",
        "    rmse = np.sqrt(np.mean((predictions - labels)**2))\n",
        "    return {\"rmse\": rmse}\n"
      ],
      "metadata": {
        "id": "_iMsdQf0AIYH"
      },
      "execution_count": 106,
